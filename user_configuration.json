{
    "additional-repositories": [],
    "archinstall-language": "English",
    "audio_config": {
        "audio": "pulseaudio"
    },
    "bootloader": "Systemd-boot",
    "config_version": "2.8.6",
    "custom-commands": [
        "git clone https://github.com/pentestfunctions/hypervarch.git /home/robot/hypervarch",
        "mkdir -p /etc/systemd/system",
        "cat > /etc/systemd/system/firstboot.service << 'EOL'\n[Unit]\nDescription=First Boot Setup Service\nAfter=network.target\nConditionPathExists=/home/robot/hypervarch/scripts\nConditionPathExists=!/var/lib/firstboot-done\n\n[Service]\nType=oneshot\nExecStart=/bin/bash -c 'cd /home/robot/hypervarch/scripts && for script in *.sh; do if [ -f \"$script\" ]; then chmod +x \"$script\" && ./\"$script\"; fi; done'\nExecStart=/bin/touch /var/lib/firstboot-done\nRemainAfterExit=yes\n\n[Install]\nWantedBy=multi-user.target\nEOL",
        "systemctl enable firstboot.service"
    ],
    "debug": false,
